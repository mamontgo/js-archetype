<?xml version="1.0" encoding="UTF-8"?>
<archetype-descriptor name="jsquickstart">
  <fileSets>
    <fileSet filtered="true" packaged="false">
      <directory>src/main/javascript</directory>
      <includes>
        <include>**/*.js</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="false">
      <directory>src/test/javascript</directory>
      <includes>
        <include>**/*.js</include>
      </includes>
    </fileSet>
    <fileSet filtered="true" packaged="v">
      <directory>helper-scripts</directory>
      <includes>
        <include>**/*</include>
      </includes>      
    </fileSet>
    <fileSet filtered="true" packaged="false">
      <directory/>
      <includes>
        <include>bower.json</include>
        <include>Gruntfile.js</include>
        <include>package.json</include>
      </includes>      
    </fileSet>
    
  </fileSets>
</archetype-descriptor>
